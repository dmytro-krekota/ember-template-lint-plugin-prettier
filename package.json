{
  "name": "ember-template-lint-plugin-prettier",
  "version": "2.0.1",
  "description": "A prettier plugin for ember-template-lint",
  "keywords": [
    "ember-template-lint",
    "ember-template-lint-plugin",
    "handlebars",
    "glimmer",
    "prettier"
  ],
  "files": [
    "ember-template-lint-plugin-prettier.js",
    "lib",
    "docs"
  ],
  "main": "ember-template-lint-plugin-prettier.js",
  "scripts": {
    "changelog": "lerna-changelog",
    "lint": "npm-run-all lint:* --continue-on-error",
    "lint:docs": "markdownlint 'docs/**/*.md'",
    "lint:js": "eslint . --cache",
    "test": "mocha --recursive",
    "format": "yarn prettier '**/*.{hbs,js,json,yml}' --write"
  },
  "repository": "git@github.com:dcyriller/ember-template-lint-plugin-prettier.git",
  "author": "Cyrille David <'cyrille@qonto.eu'>",
  "license": "Apache-2.0",
  "private": false,
  "peerDependencies": {
    "ember-template-lint": ">=2.10.0",
    "prettier": ">=1.18.1"
  },
  "devDependencies": {
    "ember-template-lint": "^3.4.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "lerna-changelog": "^1.0.0",
    "markdownlint-cli": "^0.27.1",
    "mocha": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14.*"
  },
  "dependencies": {
    "prettier-linter-helpers": "^1.0.0"
  },
  "volta": {
    "node": "12.16.3",
    "yarn": "1.22.4"
  }
}
